#include <stdio.h>

int main(void) {
    //declarar as variaveis
    int qtd_maxima, qtd_minima;
    float est_medio;

    //ler as variaveis
    printf("Digite a Quantidade Maxima\n");
    scanf("%d", &qtd_maxima);

    printf("Digite a Quantidade Minima\n");
    scanf("%d", &qtd_minima);
    est_medio = (qtd_maxima + qtd_minima) / 2;

    printf("Estoque medio: %f \n", est_medio);
    return 0;
}
#include <stdio.h>

int main() {
    int faixa;
    printf("Digite a faixa de 1 a 5: \n");
    scanf("%d", &faixa);
    switch (faixa) {
        case 1:
            printf("Crianca \n");
        break;
        case 2:
            printf("Adolescente\n");
            break;
        case 3:
            printf("Jovem\n");
            break;
        case 4:
            
    }
}
